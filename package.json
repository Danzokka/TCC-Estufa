{
  "name": "app-tcc",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "clean": "turbo run clean && rm -rf node_modules",
    "clean:all": "pnpm -r exec rm -rf node_modules && rm -rf node_modules",
    "install:clean": "pnpm clean:all && pnpm install",
    "setup": "pnpm install && pnpm run build"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "@nestjs/common": "^11.0.20",
    "@nestjs/core": "^11.0.20",
    "@nestjs/swagger": "^11.1.4",
    "web-push": "^3.6.7"
  }
}